{
    "root": true,
    "parserOptions": {
         "ecmaVersion": 12,
         "sourceType": "module"
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "env": { 
         "es2021": true,
         "node": true
    },
    "rules": {
         "no-console": 0,
         "prettier/prettier": "allow"
    }
}